{
    "name": "ibnnajjaar/helpers",
    "description": "Helper functions for personal Laravel skeleton",
    "keywords": [
        "ibnnajjaar",
        "helpers"
    ],
    "type": "library",
    "authors": [
        {
            "name": "Hussain Afeef",
            "email": "ibnahnajjaar@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "IbnNajjaar\\Helpers\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "IbnNajjaar\\Helpers\\HelpersServiceProvider"
            ]
        }
    }
}
